{
  "name": "nock-record",
  "description": "Small wrapper around Nock Back, with improved ergonomics.",
  "version": "0.3.3",
  "main": "lib/index.js",
  "typings": "lib/index.d.ts",
  "license": "MIT",
  "homepage": "https://github.com/edorivai/nock-record",
  "repository": "github:edorivai/nock-record",
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "testURL": "http://localhost/"
  },
  "files": [
    "lib"
  ],
  "scripts": {
    "build": "tsc",
    "test": "jest test",
    "test-examples": "jest examples",
    "precommit": "lint-staged"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "prettier --parser typescript --write",
      "git add"
    ]
  },
  "devDependencies": {
    "@types/jest": "^24.9.0",
    "@types/nock": "^11.1.0",
    "@types/node": "^12.7.3",
    "@types/stack-trace": "^0.0.29",
    "@types/superagent": "^4.1.3",
    "husky": "^0.14.3",
    "jest": "^24.9.0",
    "lint-staged": "^8.1.3",
    "prettier": "^1.19.1",
    "superagent": "^3.8.3",
    "ts-jest": "^24.3.0",
    "ts-node": "^5.0.1",
    "typescript": "^3.6.2"
  },
  "dependencies": {
    "nock": "^11.7.2",
    "stack-trace": "^0.0.10"
  }
}
